                "operationId": "loginWithOtp",
                "requestBody": {
                    "content": {
                        "application/x-www-form-urlencoded": {
                            "schema": {
                                "properties": {
                                    "phone_number": {
                                        "type": "string",
                                        "example": "+213664419425"
                                    },
                                    "otp": {
                                        "type": "string",
                                        "example": "55555"
