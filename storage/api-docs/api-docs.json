        "/api/v1/patients/complete-medical-record": {
            "post": {
                "tags": [
                    "patients"
                ],
                "summary": "update or complete patient medical record",
                "description": "update or complete patient medical record ",
                "operationId": "storePatientData",
                "requestBody": {
                    "content": {
                        "application/x-www-form-urlencoded": {
                            "schema": {
                                "properties": {
                                    "full_name": {
                                        "type": "string",
                                        "example": "ahmed amine"
                                    },
                                    "birth_date": {
                                        "type": "string",
                                        "example": "25-05-1995"
                                    },
                                    "age": {
                                        "type": "integer",
                                        "example": 28
                                    },
                                    "gender": {
                                        "type": "integer",
                                        "example": "male"
                                    },
                                    "address": {
                                        "type": "integer",
                                        "example": "adrar alg"
                                    },
                                    "height": {
                                        "type": "integer",
                                        "example": 180
                                    },
                                    "weight": {
                                        "type": "number",
                                        "format": "double",
                                        "example": "55.5"
                                    },
                                    "allergy_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "chronic_diseases_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "family_history_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "has_cancer_screening": {
                                        "type": "boolean",
                                        "enum": [
                                            0,
                                            1
                                        ]
                                    },
                                    "has_depression_screening": {
                                        "type": "boolean",
                                        "enum": [
                                            0,
                                            1
                                        ]
                                    },
                                    "other_problems": {
                                        "type": "object"
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "application/json": {
                            "schema": {}
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Patient Record Completed Successfuly",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "404": {
                        "description": "Patient not found with the given token or phone number, please login again",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "500": {
                        "description": "internal server error",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "401": {
                        "description": "unauthenticated",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
